<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <RootNamespace>BCCWords</RootNamespace>
    <AssemblyName>BCCWords</AssemblyName>
    <Nullable>disable</Nullable>
    <ImplicitUsings>disable</ImplicitUsings>
    <LangVersion>latest</LangVersion>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.ML.Probabilistic" Version="0.4.2402.2904" />
    <PackageReference Include="Microsoft.ML.Probabilistic.Compiler" Version="0.4.2402.2904" />
    <PackageReference Include="StemmersNet" Version="1.0.0" />
  </ItemGroup>

</Project>